{
    "name": "lechihuy/bit-laravel-skeleton",
    "description": "Building the Laravel Skeleton for your application was inspired by Lucid architecture.",
    "homepage": "https://github.com/lechihuy/bit-laravel-skeleton",
    "authors": [
        {
            "name": "Lê Chí Huy",
            "email": "lechihuy1062001@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Bit\\Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bit\\Skeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bit\\Skeleton\\Providers\\SkeletonServiceProvider"
            ]
        }
    }
}